use "std"

// header
print " " * 4
for b : range(9)
  print sprintf("  4%dm ", b)
println ""

for f : range(30, 39) {
  for s : ["", "1;"] {
    print sprintf("%4sm", s+f)
    print sprintf(" \u001B[%sm%s\u001B[0m", s+f, "gYw ")
    for b : range(8)
      print sprintf(" \u001B[4%s;%sm%s\u001B[0m", b, s+f, " gYw ")
    println ""
  }
}

