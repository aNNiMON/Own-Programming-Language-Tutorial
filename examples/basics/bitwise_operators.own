use std

echo(#ABCDEF - #12345)
echo(-8 << 2)
echo(-8 >> 2)
echo(-8 >>> 2)

echo()
for a = 0, a <= 1, a++ {
    for b = 0, b <= 1, b++ {
        echo(a, " | ", b, " ", a | b)
        echo(a, " & ", b, " ", a & b)
        echo(a, " ^ ", b, " ", a ^ b)
    }
    echo("   ~", a, " ", ~a)
    echo("   !", a, " ", !a)
}